{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Basic webpack config to init projects",
  "main": "index.js",
  "scripts": {
    "prod": "npm run clean && NODE_ENV=production webpack -p",
    "clean": "rimraf ./build/*",
    "compile": "webpack --config webpack.config.js",
    "server": "cross-env NODE_ENV=development WEBPACK_CONFIG=./webpack.config.js node server.js"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "project"
  ],
  "author": "Antonio Junior",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.3.1",
    "morgan": "^1.9.0",
    "node-emoji": "^1.8.1",
    "node-sass": "^4.5.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^2.0.0",
    "react-router-dom": "^4.1.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "whatwg-fetch": "^2.0.3"
  }
}
